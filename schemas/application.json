{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://api.kohost.app/schemas/v3/application.json",
  "title": "Application",
  "type": "object",
  "default": {},
  "required": [
    "id",
    "active",
    "name",
    "phone",
    "publicKey",
    "hashedPrivateKey"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "publicKey": {
      "type": "string"
    },
    "hashedPrivateKey": {
      "type": "string"
    }
  }
}
