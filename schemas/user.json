{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": ["active", "firstName", "lastName", "phone", "email"],
  "properties": {
    "id": {},
    "active": {
      "title": "The active Schema",
      "type": "boolean",
      "default": false,
      "examples": [true]
    },
    "firstName": {
      "title": "The firstName Schema",
      "type": "string",
      "default": "",
      "examples": ["Kohost"]
    },
    "lastName": {
      "title": "The lastName Schema",
      "type": "string",
      "default": "",
      "examples": ["User"]
    },
    "phone": {
      "title": "The phone Schema",
      "type": "string",
      "default": "",
      "examples": ["+17022917409"]
    },
    "email": {
      "title": "The email Schema",
      "type": "string",
      "default": "",
      "examples": ["admin@kohost.io"]
    }
  },
  "examples": [
    {
      "active": true,
      "firstName": "Kohost",
      "lastName": "User",
      "phone": "+17022917409",
      "email": "admin@kohost.io"
    }
  ]
}
