{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "log.json",
  "title": "Log",
  "type": "object",
  "required": ["id", "timestamp"],
  "properties": {
    "id": {
      "$ref": "definitions.json#/definitions/id"
    },
    "type": {
      "type": "string",
      "default": "log",
      "enum": ["log"]
    },
    "timestamp": {
      "type": "number",
      "minimum": 1655907956593
    },
    "driver": {
      "$ref": "definitions.json#/definitions/driver"
    },
    "message": {
      "type": "string"
    },
    "field1": {
      "type":"string"
    },
    "field2": {
      "type":"string"
    },
    "field3": {
      "type":"string"
    },
    "field4": {
      "type":"string"
    }
  }
}
