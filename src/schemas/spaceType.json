{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "spaceType.json",
  "title": "Space Type",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "definitions.json#/definitions/id"
    },
    "type": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "driver": {
      "$ref": "definitions.json#/definitions/driver"
    },
    "description": {
      "type": "string"
    },
    "image": {
      "$ref": "mediaFile.json"
    },
    "rating": {
      "type": "number",
      "minimum": 0,
      "maximum": 10,
      "default": 9
    },
    "systemData": {
      "$ref": "definitions.json#/definitions/systemData"
    }
  }
}
