{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "credential.json",
  "title": "Credential",
  "type": "object",
  "required": ["type", "credential", "expires"],
  "properties": {
    "id": {
      "$ref": "definitions.json#/definitions/id"
    },
    "type": {
      "type": "string",
      "default": "credential"
    },
    "discriminator": {
      "type": "string",
      "enum": ["verificationCode", "token", "mobileKey"]
    },
    "credential": {
      "type": "string"
    },
    "user": {
      "type": "string"
    },
    "organization": {
      "type": "string"
    },
    "property": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "expires": {
      "string": "string"
    },
    "systemId": {
      "$ref": "definitions.json#/definitions/systemId"
    }
  }
}
