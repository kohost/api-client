{
  "name": "@kohost/api-client",
  "version": "4.0.7",
  "description": "API client, models, schemas, commands, and events for Kohost applications",
  "author": "Ian Rogers",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohost/api-client.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "node": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./models": {
      "types": "./dist/models.d.ts",
      "import": "./dist/models.js",
      "require": "./dist/models.cjs"
    },
    "./useCases": {
      "types": "./dist/useCases.d.ts",
      "import": "./dist/useCases.js",
      "require": "./dist/useCases.cjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.cjs"
    },
    "./commands": {
      "types": "./dist/commands.d.ts",
      "import": "./dist/commands.js",
      "require": "./dist/commands.cjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "require": "./dist/events.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./socketIoClient": {
      "types": "./dist/socketIoClient.d.ts",
      "import": "./dist/socketIoClient.js",
      "require": "./dist/socketIoClient.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint",
    "lint:fix": "eslint --fix ./",
    "prepare": "husky",
    "prebuild": "npm run generate-models && npm run generate-barrels",
    "build": "tsup",
    "generate-models": "node scripts/generate-source-models",
    "generate-barrels": "node scripts/generate-barrels",
    "start": "node scripts/build.js --watch",
    "clean": "rm -rf ./dist",
    "tsc": "tsc",
    "tsup": "tsup"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/debug": "^4.1.12",
    "@types/events": "^3.0.3",
    "@types/node": "^22.10.5",
    "ajv": "^8.11.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "debug": "^4.3.4",
    "socket.io-client": "^4.6.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json}": "prettier --write"
  }
}
