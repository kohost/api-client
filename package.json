{
  "name": "@kohost/api-client",
  "version": "3.0.0-beta.53",
  "description": "API client, models, schemas, commands, and events for Kohost applications",
  "author": "Ian Rogers",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/kohost/api-client"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./Models/*": {
      "import": "./dist/esm/Models/*.js",
      "types": "./dist/esm/Models/*.d.ts",
      "require": "./dist/cjs/Models/*.js"
    },
    "./Commands/*": {
      "import": "./dist/esm/Commands/*.js",
      "require": "./dist/cjs/Commands/*.js"
    },
    "./Events/*": {
      "import": "./dist/esm/Events/*.js",
      "require": "./dist/cjs/Events/*.js"
    },
    "./Errors/*": {
      "import": "./dist/esm/Errors/*.js",
      "require": "./dist/cjs/Errors/*.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepare": "node prepare.js",
    "build": "node scripts/build.js",
    "build:watch": "node scripts/build.js --watch",
    "start": "node scripts/build.js --watch",
    "clean": "rm -rf ./dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@kohost/eslint-config": "^1.0.0",
    "dts-gen": "^0.6.1",
    "esbuild": "^0.18.12",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^27.1.3",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "json-schema-to-typescript": "^13.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@kohost/api-client": "^3.0.0-beta.52",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "amqplib": "^0.10.3",
    "axios": "^1.0.0",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.findlast": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "nanoid": "^3.3.4",
    "socket.io-client": "^4.6.1"
  },
  "browser": {
    "events": "events"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json}": "prettier --write"
  }
}
