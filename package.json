{
  "name": "@kohost/api-client",
  "version": "4.20.2",
  "description": "API client, models, schemas, commands, and events for Kohost applications",
  "author": "Ian Rogers",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohost/api-client.git"
  },
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./schemas/*": {
      "import": {
        "default": "./dist/esm/schemas/*.ts"
      }
    },
    "./models": {
      "import": {
        "types": "./dist/esm/models/index.d.ts",
        "default": "./dist/esm/models/index.js"
      },
      "require": {
        "types": "./dist/cjs/models/index.d.cts",
        "default": "./dist/cjs/models/index.cjs"
      }
    },
    "./models/*": {
      "import": {
        "types": "./dist/esm/models/*.d.ts",
        "default": "./dist/esm/models/*.js"
      },
      "require": {
        "types": "./dist/cjs/models/*.d.cts",
        "default": "./dist/cjs/models/*.cjs"
      }
    },
    "./useCases": {
      "import": {
        "types": "./dist/esm/useCases/index.d.ts",
        "default": "./dist/esm/useCases/index.js"
      },
      "require": {
        "types": "./dist/cjs/useCases/index.d.cts",
        "default": "./dist/cjs/useCases/index.cjs"
      }
    },
    "./useCases/*": {
      "import": {
        "types": "./dist/esm/useCases/*.d.ts",
        "default": "./dist/esm/useCases/*.js"
      },
      "require": {
        "types": "./dist/cjs/useCases/*.d.cts",
        "default": "./dist/cjs/useCases/*.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/esm/errors/index.d.ts",
        "default": "./dist/esm/errors/index.js"
      },
      "require": {
        "types": "./dist/cjs/errors/index.d.cts",
        "default": "./dist/cjs/errors/index.cjs"
      }
    },
    "./errors/*": {
      "import": {
        "types": "./dist/esm/errors/*.d.ts",
        "default": "./dist/esm/errors/*.js"
      },
      "require": {
        "types": "./dist/cjs/errors/*.d.cts",
        "default": "./dist/cjs/errors/*.cjs"
      }
    },
    "./commands": {
      "import": {
        "types": "./dist/esm/commands/index.d.ts",
        "default": "./dist/esm/commands/index.js"
      },
      "require": {
        "types": "./dist/cjs/commands/index.d.cts",
        "default": "./dist/cjs/commands/index.cjs"
      }
    },
    "./commands/*": {
      "import": {
        "types": "./dist/esm/commands/*.d.ts",
        "default": "./dist/esm/commands/*.js"
      },
      "require": {
        "types": "./dist/cjs/commands/*.d.cts",
        "default": "./dist/cjs/commands/*.cjs"
      }
    },
    "./events": {
      "import": {
        "types": "./dist/esm/events/index.d.ts",
        "default": "./dist/esm/events/index.js"
      },
      "require": {
        "types": "./dist/cjs/events/index.d.cts",
        "default": "./dist/cjs/events/index.cjs"
      }
    },
    "./events/*": {
      "import": {
        "types": "./dist/esm/events/*.d.ts",
        "default": "./dist/esm/events/*.js"
      },
      "require": {
        "types": "./dist/cjs/events/*.d.cts",
        "default": "./dist/cjs/events/*.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/esm/httpClient.d.ts",
        "default": "./dist/esm/httpClient.js"
      },
      "require": {
        "types": "./dist/cjs/httpClient.d.cts",
        "default": "./dist/cjs/httpClient.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/esm/*"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint",
    "lint:fix": "eslint --fix ./",
    "prepare": "husky",
    "prebuild": "npm run generate-models",
    "build": "tsup",
    "generate-models": "tsx scripts/generate-source-models.js",
    "clean:generated": "rm -rf .generated",
    "start": "node scripts/build.js --watch",
    "clean": "rm -rf ./dist ./dist-cjs ./dist-esm",
    "tsc": "tsc",
    "tsup": "tsup"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/events": "^3.0.3",
    "@types/node": "^22.10.5",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.0.11",
    "json-schema-to-ts": "^3.1.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.3",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^3.0.1",
    "amqplib": "^0.10.3",
    "debug": "^4.3.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json}": "prettier --write"
  }
}
